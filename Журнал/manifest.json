{
  "name": "Журнал by Max Zayats",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["jquery-3.1.0.min.js","main.js"]
      }
  ],
  "permissions": [
      "activeTab"
  ]
}
